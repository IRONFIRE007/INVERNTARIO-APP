            <input type="text" class="" name="telefono"  placeholder="Telefono" autocomplete="off">
          </div>
    
          <div class="form-floating">
            <button type="submit" class="w-100 btn btn-lg ">crear</button>
          </div>
      </form>
    
    </main>

  

